{
    "manifest_version" :2,
    "name":"LightWeight Stopwatch",
    "version":"1.0",
    "description":"A LightWeight Stopwatch for personal use!",
    "icons":
    {
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "browser_action":{
"default_icon":"icon128.png",
"default_popup":"default.html"
    },
  "background": {
        "scripts": [
          "background.js"
        ]
      },
    "permissions": [
        "storage"
      ],
      "key": ""
}